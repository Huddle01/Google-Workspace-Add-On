{
  "timeZone": "Asia/Kolkata",
  "dependencies": {
    "enabledAdvancedServices": [
      {
        "userSymbol": "Gmail",
        "version": "v1",
        "serviceId": "gmail"
      },
      {
        "userSymbol": "Calendar",
        "version": "v3",
        "serviceId": "calendar"
      }
    ]
  },
  "addOns": {
    "common": {
      "name": "Huddle01 for Google Workspace",
      "logoUrl": "https://dy95p8aq490x0.cloudfront.net/huddle01_logo.png",
      "useLocaleFromApp": true,
      "homepageTrigger": {
        "runFunction": "onHomepage",
        "enabled": true
      },
      "universalActions": [
        {
          "label": "Visit Huddle01",
          "openLink": "https://huddle01.com"
        }
      ]
    },
    "calendar": {
      "conferenceSolution": [
        {
          "id": 1,
          "name": "Huddle01 Meeting",
          "logoUrl": "https://dy95p8aq490x0.cloudfront.net/huddle01_logo.png",
          "onCreateFunction": "createConference"
        }
      ]
    },
    "gmail": {
      "contextualTriggers": [
        {
          "unconditional": {},
          "onTriggerFunction": "onGmailMessageOpen"
        }
      ],
      "composeTrigger": {
        "selectActions": [
          {
            "text": "Insert cat",
            "runFunction": "onGmailCompose"
          }
        ],
        "draftAccess": "NONE"
      }
    }
  },
  "oauthScopes": [
    "https://www.googleapis.com/auth/calendar.events",
    "https://www.googleapis.com/auth/calendar.addons.execute",
    "https://www.googleapis.com/auth/script.external_request",
    "https://www.googleapis.com/auth/gmail.addons.current.action.compose",
    "https://www.googleapis.com/auth/gmail.addons.current.message.readonly",
    "https://www.googleapis.com/auth/gmail.addons.execute",
    "https://www.googleapis.com/auth/script.locale",
    "https://www.googleapis.com/auth/userinfo.email"
  ],
  "urlFetchWhitelist": [
    "https://wpss2zlpb9.execute-api.us-east-1.amazonaws.com/new-meeting",
    "https://us-central1-nfts-apis.cloudfunctions.net/createroom",
    "https://platform-api.huddle01.workers.dev/api/v2/calendar/subdomains",
    "https://platform-api.huddle01.workers.dev/api/v2/calendar/createMeeting",
    "https://huddle01.app/thirdparty_auth"
  ],
  "exceptionLogging": "STACKDRIVER",
  "runtimeVersion": "V8"
}
